#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h> 
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/mouse.h>
#include <dt-bindings/zmk/outputs.h>

#define DEFAULT 0
#define QWERTY  1
#define MOUSE2  2
#define GAME    3
#define MOUSE   4
#define NAV     5
#define TOGGLE  6

//Mouse Movement
#define MUP       MOVE(0,     -800)
#define MLEFT     MOVE(-800, 0)
#define MDOWN     MOVE(0,     800) 
#define MRIGHT    MOVE(800,  0) 
#define MSUP      SCROLL_UP
#define MSDOWN    SCROLL_DOWN

#define SEARCH    LS(F12)
#define JOURNAL   F21

//Ctrl + Arrows
#define CUP     LS(LC(T))
#define CLEFT   LS(LC(TAB))
#define CDOWN   LC(W)
#define CRIGHT  LC(TAB)

//╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────────╮
